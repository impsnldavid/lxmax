<c74object name='lx.colorfixture' category='lxmax'>

	<digest>Send DMX control data to one or more dimmers</digest>
	<description>Send DMX control data to one or more dimmers. </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>David Butler / The Impersonal Stereo </metadata>
		<metadata name='tag'>lxmax</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='channel' optional='1' type='number'>
			<digest>DMX start channel for dimmer(s) </digest>
			<description>DMX start channel for dimmer(s) </description>
		</objarg>

		<objarg name='universe' optional='1' type='number'>
			<digest>DMX universe number </digest>
			<description>DMX universe number </description>
		</objarg>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

	</methodlist>


